    <!-- <offering profile='IBM HTTP Server V9.0' id='com.ibm.websphere.PLG.v90' version='9.0.0.20160526_1854' features='core.feature'/> -->
    <offering id='com.ibm.websphere.IHS.v85' 
      profile='IBM HTTP Server V8.5' installFixes='none'/>
    <offering id='com.ibm.websphere.PLG.v85' 
      profile='Web Server Plug-ins for IBM WebSphere Application Server V8.5'  installFixes='none'/>

  </install>
  <preference name='com.ibm.cic.common.core.preferences.eclipseCache' value='/opt/IBM/IMShared'/>
