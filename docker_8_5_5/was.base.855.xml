  <profile id='Web Server Plug-ins for IBM WebSphere Application Server V8.5' 
    installLocation='/opt/IBM/WebSphere/Plugins'>
    <data key='eclipseLocation' value='/opt/IBM/WebSphere/Plugins'/>
    <data key='user.import.profile' value='false'/>
    <data key='cic.selector.nl' value='en'/>
  </profile>

  <install>
    <!-- IBM WebSphere Application Server  9.0.0.8 -->
    <offering profile='IBM WebSphere Application Server V8.5' id='com.ibm.websphere.BASE.v85' version='8.5.5015.20190128_1828' features='core.feature,ejbdeploy,thinclient,embeddablecontainer,com.ibm.sdk.6_64bit'/>
